{
	"name" : "iu-redcap/phpcap",
	"license" : "BSD-3-Clause",
	"description" : "PHP library for accessing the REDCap API",
	"keywords" : [
		"REDCap"
	],
	"require" : {
		"php" : ">=5.6.0",
		"ext-curl" : "*",
		"ext-openssl" : "*"
	},
	"require-dev" : {
		"phpunit/phpunit" : "11.0.4",
		"squizlabs/php_codesniffer" : "3.9.0",
		"erusev/parsedown" : "1.7.4",
        "smalot/pdfparser" : "2.9.0"
	},
	"autoload" : {
		"psr-4" : {
			"IU\\PHPCap\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"IU\\PHPCap\\" : [
				"tests/unit",
				"tests/integration",
				"tests/util"
			]
		},
		"files" : [
			"tests/integration/SystemFunctions.php"
		]
	}
}
