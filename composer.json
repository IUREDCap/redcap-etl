{
	"description" : "REDCap ETL",
	"keywords" : [
		"REDCap", "ETL"
	],
	"require" : {
		"php" : ">=5.6.0",
		"ext-curl" : "*",
        "ext-dom" : "*",
		"ext-openssl" : "*",
		"phpcap/phpcap" : "^0.3.2"
	},
        "require-dev" : {
        "apigen/apigen" : "v4.1.2",
		"phpunit/phpunit" : "6.3.0",
        "squizlabs/php_codesniffer" : "2.8.1",
		"kzykhys/php-csv-parser" : "1.4.1"
        },
	"autoload" : {
		"psr-4" : {
			"IU\\REDCapETL\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"IU\\REDCapETL\\" : [
				"tests/unit",
				"tests/integration",
				"tests/system"
			]
		}
	}
}

