{
    "name" : "iu-redcap/redcap-etl",
    "license" : "BSD-3-Clause",
    "description" : "ETL (Extract, Transform, Load) application that extracts data from REDCap",
    "keywords" : [
        "REDCap", "ETL"
    ],
    "require" : {
        "php" : ">=5.6.0",
        "ext-curl" : "*",
        "ext-dom" : "*",
        "ext-openssl" : "*",
        "iu-redcap/phpcap" : "1.5.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "11.0.0",
        "squizlabs/php_codesniffer" : "3.9.0"
    },
    "autoload" : {
        "psr-4" : {
            "IU\\REDCapETL\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "IU\\REDCapETL\\" : [
                "tests/unit",
                "tests/integration",
                "tests/system",
                "tests/util"
            ]
        }
    }
}
