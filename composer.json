{
    "name" : "iu-redcap/redcap-etl",
    "license" : "BSD-3-Clause",
    "description" : "ETL (Extract, Transform, Load) application that extracts data from REDCap",
    "keywords" : [
        "REDCap", "ETL"
    ],
    "require" : {
        "php" : ">=5.6.0",
        "ext-curl" : "*",
        "ext-dom" : "*",
        "ext-openssl" : "*",
        "iu-redcap/phpcap" : "1.0.5"
    },
    "require-dev" : {
        "apigen/apigen" : "v4.1.2",
        "phpunit/phpunit" : "8.5.14",
        "squizlabs/php_codesniffer" : "3.5.8",
        "kzykhys/php-csv-parser" : "1.4.1"
    },
    "autoload" : {
        "psr-4" : {
            "IU\\REDCapETL\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "IU\\REDCapETL\\" : [
                "tests/unit",
                "tests/integration",
                "tests/system"
            ]
        }
    }
}

